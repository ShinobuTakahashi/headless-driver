{
  "name": "@akashic/headless-driver",
  "version": "0.4.0",
  "description": "A library to execute contents using Akashic Engine headlessly",
  "main": "lib/index.js",
  "author": "DWANGO Co., Ltd.",
  "license": "MIT",
  "typings": "lib/index.d.ts",
  "scripts": {
    "prepublish": "npm run build",
    "build": "rimraf ./lib && tsc -p ./tsconfig.json && npm run formatter",
    "formatter": "prettier --write \"src/**/*.ts\" --config ../prettier.config.js",
    "test": "npm run build && npm run run:jest && npm run lint",
    "run:jest": "jest",
    "lint": "tslint -c ../tslint.json src/**/*.ts -p tsconfig.json -e \"**/node_modules/**\""
  },
  "publishConfig": {
    "access": "public",
    "@akashic:registry": "https://registry.npmjs.org/"
  },
  "files": [
    "lib"
  ],
  "dependencies": {
    "@akashic/amflow": "0.2.2",
    "@akashic/headless-driver-runner": "0.4.0",
    "@akashic/headless-driver-runner-v1": "0.4.0",
    "@akashic/headless-driver-runner-v2": "0.4.0",
    "@akashic/playlog": "1.3.1",
    "@akashic/trigger": "0.1.5",
    "js-sha256": "0.9.0",
    "lodash.clonedeep": "4.5.0",
    "node-fetch": "2.3.0"
  },
  "devDependencies": {
    "@types/get-port": "4.0.0",
    "@types/http-server": "0.10.0",
    "@types/jest": "24.0.12",
    "@types/lodash.clonedeep": "4.5.4",
    "@types/node-fetch": "2.1.2",
    "get-port": "^5.0.0",
    "http-server": "^0.11.1",
    "jest": "^24.8.0",
    "prettier": "^1.17.0",
    "rimraf": "^2.6.3",
    "ts-jest": "^24.0.2",
    "tslint": "^5.16.0",
    "typescript": "^3.4.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "tsConfig": "tsconfig.json"
      }
    },
    "testMatch": [
      "**/__tests__/*.ts"
    ]
  }
}
