{
  "name": "@akashic/headless-driver-runner-v2",
  "version": "1.8.0",
  "description": "A runner module for contents using Akashic Engine v2",
  "main": "lib/index.js",
  "scripts": {
    "prepublish": "npm run build",
    "build": "rimraf ./lib && tsc -p ./tsconfig.json && npm run formatter",
    "formatter": "prettier --write \"src/**/*.ts\" --config ../prettier.config.js",
    "test": "npm run lint",
    "lint": "eslint \"src/**/*.ts\" --fix"
  },
  "publishConfig": {
    "access": "public",
    "@akashic:registry": "https://registry.npmjs.org/"
  },
  "files": [
    "lib"
  ],
  "author": "DWANGO Co., Ltd.",
  "license": "MIT",
  "typings": "lib/index.d.ts",
  "dependencies": {
    "@akashic/engine-files": "2.1.55",
    "@akashic/headless-driver-runner": "1.8.0"
  },
  "devDependencies": {
    "@akashic/eslint-config": "0.1.2",
    "@typescript-eslint/eslint-plugin": "4.19.0",
    "eslint": "7.22.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jest": "24.3.2",
    "prettier": "2.2.1",
    "rimraf": "3.0.2",
    "tslint": "6.1.3",
    "typescript": "4.2.3"
  }
}
